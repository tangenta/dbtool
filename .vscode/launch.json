    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go",
            "args": [
                "bench", "sysbench", "--tables=100", "--rows=10000",
            ]
        },
        {
            "name": "Run Precheck",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go",
            "args": [
                "precheck", "-f", "./bin/precheck.sql",
            ]
        },
        {
            "name": "Run Metacli",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go",
            "args": [
                "metacli", 
                "--config", "/home/tangenta/tidb/pkg/config/config.toml.example",
                "--store", "tikv",
                "--path", "127.0.0.1:2379",
                "--op", "get",
                "--key", "max-ingest-inflight",
                "--value", "16",
            ]
        }
    ]
}